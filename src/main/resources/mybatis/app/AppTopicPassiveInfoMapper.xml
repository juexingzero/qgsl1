<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manhui.gsl.jbqgsl.dao.app.AppTopicPassiveInfoMapper">
	<select id="queryInstitutionsInfo" resultType="string">
			select 
				institution_id 
			from 
				topic_passive_info 
			where
				topic_id = #{topic_id}
	</select>
	
	
</mapper>